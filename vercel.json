{
  "version": 2,
  "public": true,
  "buildCommand": "npm run build",
  "outputDirectory": "docs",
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
