{
  "version": 2,
  "public": true,
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**",
          "docs/**",
          "!docs/textures/**",
          "!docs/models/**",
          "!docs/uploads/**"
        ]
      }
    },
    {
      "src": "docs/**",
      "use": "@vercel/static",
      "config": {
        "includeFiles": [
          "dist/**",
          "docs/**",
          "!docs/textures/**",
          "!docs/models/**",
          "!docs/uploads/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
